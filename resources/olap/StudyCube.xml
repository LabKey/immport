<Schema name="ImmPort" description="ImmPort study browser">

   <Cube name="StudyCube" visible="true" defaultMeasure="RowCount" cache="true" enabled="true">

        <Table schema="immport" name="dimStudy" >
        </Table>

        <Dimension visible="true" highCardinality="false" name="Subject" foreignKey="participantid">
            <Hierarchy visible="true" hasAll="true" uniqueKeyLevelName="Name" primaryKey="participantid">
                <Table name="dimDemographic" schema="immport"/>
                <Level name="Subject" visible="true" column="participantid" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
                </Level>
            </Hierarchy>
            <Hierarchy name="Race" visible="true" hasAll="true" uniqueKeyLevelName="Name" primaryKey="participantid">
                <Table name="dimDemographic" schema="immport"/>
                <Level name="Race" visible="true" column="race" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
\            </Hierarchy>
            <Hierarchy name="Gender" visible="true" hasAll="true" uniqueKeyLevelName="Name" primaryKey="participantid">
                <Table name="dimDemographic" schema="immport"/>
                <Level name="Gender" visible="true" column="gender" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
            <Hierarchy name="Species" visible="true" hasAll="true" uniqueKeyLevelName="Name" primaryKey="participantid">
                <Table name="dimDemographic" schema="immport"/>
                <Level name="Species" visible="true" column="species" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
        </Dimension>

        <Dimension name="Study" visible="true" highCardinality="false" foreignKey="study">
            <Hierarchy hasAll="true">
                <Level name="Name" visible="true" nameColumn="study" column="sortOrder" type="Integer" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
            <Hierarchy name="Program" hasAll="true">
                <Level name="Program" visible="true" column="program" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
            <Hierarchy name="Type" hasAll="true">
                <Level name="Type" visible="true" column="type" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
            <Hierarchy name="Conditions" hasAll="true" primaryKey="study">
                <Table name="dimStudyCondition" schema="immport"/>
                <Level name="Condition" visible="true" table="dimStudyCondition" column="condition" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
            <Hierarchy name="Principal" hasAll="true" primaryKey="study">
                <Table name="dimStudyPrincipals" schema="immport"/>
                <Level name="Principal" visible="true" table="dimStudyPrincipals" type="String" column="full_name" ordinalColumn="sortorder" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
                </Level>
            </Hierarchy>
        </Dimension>

        <Dimension name="Timepoint" visible="true" highCardinality="false" foreignKey="study">
            <Hierarchy name="Timepoints" hasAll="true" primaryKey="study">
                <Table name="dimStudyTimepoint" schema="immport"/>
                <Level name="Timepoint" visible="true" table="dimStudyTimepoint" column="timepoint" ordinalColumn="sortorder" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never">
                </Level>
            </Hierarchy>
        </Dimension>


        <Dimension name="Assay" visible="true" highCardinality="false" foreignKey="study">
            <Hierarchy hasAll="true" primaryKey="study">
                <Table name="dimAssay" schema="immport"/>
                <Level name="Assay" visible="true" table="dimAssay" column="assay" type="String" uniqueMembers="true" levelType="Regular" hideMemberIf="Never"/>
            </Hierarchy>
        </Dimension>


        <Measure name="RowCount" column="participantid" aggregator="count" visible="true">
        </Measure>
    </Cube>


</Schema>
